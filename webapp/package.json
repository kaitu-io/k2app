{
  "name": "k2-webapp",
  "private": true,
  "version": "0.3.22",
  "type": "module",
  "exports": {
    "./src/*": "./src/*"
  },
  "scripts": {
    "dev": "vite",
    "build": "node ../scripts/check-i18n.mjs --ci && tsc && vite build",
    "check-i18n": "node ../scripts/check-i18n.mjs",
    "build:openwrt": "vite build --config vite.config.openwrt.ts",
    "dev:openwrt": "vite --config vite.config.openwrt.ts",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:webkit-input": "playwright test --project=webkit-input-tests",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:e2e:flows": "playwright test e2e/flows/",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install --with-deps"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@sentry/react": "^10.3.0",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-http": "^2.5.7",
    "@tauri-apps/plugin-shell": "^2.3.5",
    "country-flag-icons": "^1.5.19",
    "i18n-iso-countries": "^7.14.0",
    "i18next": "^25.3.6",
    "i18next-browser-languagedetector": "^8.2.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^7.6.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.15.29",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^25.0.1",
    "terser": "^5.44.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  }
}
